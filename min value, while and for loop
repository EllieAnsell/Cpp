#include <iostream>
#include <cstdlib>

int main()
{
    for(int i=18;i<26;i++) //ex01
    {      
        std::cout<<i<<"\n";
    }
    double i=2.00;
    while(i<=10) //ex02
    {
        std::cout<<"\n i is:"<<i;
        i+=1;
    }    
    int smallestInt[]={9,5,7,3,6};
    int min=5;
    std::cout<<min<<std::endl;
    for(int i=0;i<5;i++)
    {
        if(min>smallestInt[i])
        {
            min=smallestInt[i];
        }
    }
    std::cout<<min<<std::endl;
    {
        std::cout<<min<<"minimum value"<<std::endl;
        std::cout<<smallestInt<<std::endl;
    }
    return EXIT_SUCCESS;
}
